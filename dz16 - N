using System;

internal class Program
{
    private static void Main(string[] args)
    {
        const int BottomLimiter = 99;
        const int TopLimiter = 1000;

        Random random = new Random();
        int number = random.Next(0, 28);
        int dividentsCount = 0;
        int sum = 0;

        while (sum < BottomLimiter)
        {
            sum += number;
        }
        while (sum < TopLimiter)
        {
            sum += number;
            dividentsCount++;
        }

        Console.WriteLine($"Количество чисел кратных {number} равно: {dividentsCount}");
    }
}
