using System;

internal class Program
{
    private static void Main(string[] args)
    {
        int number;
        int minValue = 0;
        int maxValue = 101;
        int multiplicityCondition3 = 3;
        int multiplicityCondition5 = 5;
        int sum = 0;

        Random rand = new Random();

        number = rand.Next(minValue, maxValue);
        Console.WriteLine(number);

        for (int i = minValue; i <= number; i++)
        {
            if (i % multiplicityCondition3 == 0 || i % multiplicityCondition5 == 0)
            {
                sum += i;
            }
        }

        Console.WriteLine(sum);
    }
}
