using System;

internal class Program
{
    private static void Main(string[] args)
    {
        string name = "Parker";
        string surname = "Peter";

        Console.WriteLine("Name: " + name);
        Console.WriteLine("Surname: " + surname);

        string temporaryStorage = name;

        name = surname;
        surname = temporaryStorage;

        Console.WriteLine("Name: " + name);
        Console.WriteLine("Surname: " + surname);
    }
}
